{
  "expo": {
    "name": "HealingHub",
    "slug": "HealingHub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "experiments": {
      "typedRoutes": true
    },
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#20284F"
    },
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jaehongpark.HealingHub"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "@react-native-seoul/kakao-login",
        {
          "kakaoAppKey": "e6b1b3f5e6e616b6f75e926f9514ba8e",
          "overrideKakaoSDKVersion": "2.11.2",
          "kotlinVersion": "1.9.0"
        }
      ],
      [
        "expo-asset",
        {
          "assets": [
            "./assets/images/collagen.png",
            "./assets/images/digestion.png",
            "./assets/images/immune.png",
            "./assets/images/joint.png",
            "./assets/images/omega3.png",
            "./assets/images/others.png",
            "./assets/images/vitamin.png",
            "./assets/images/shop.jpg"
          ]
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "extraMavenRepos": ["https://devrepo.kakao.com/nexus/content/groups/public/"]
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-build-properties"
    ],
    "extra": {
      "eas": {
        "projectId": "a99a8002-5afe-4b34-bbab-7c8ef1c9e195"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/a99a8002-5afe-4b34-bbab-7c8ef1c9e195"
    }
  }
}
