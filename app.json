{
  "expo": {
    "name": "HealingHub",
    "slug": "HealingHub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "experiments": {
      "typedRoutes": true
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.jaehongpark.HealingHub"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jaehongpark.HealingHub",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-asset",
        {
          "assets": [
            "./assets/images/collagen.png",
            "./assets/images/digestion.png",
            "./assets/images/immune.png",
            "./assets/images/joint.png",
            "./assets/images/omega3.png",
            "./assets/images/others.png",
            "./assets/images/vitamin.png",
            "./assets/images/shop.jpg",
            "./assets/images/loading.png",
            "./assets/images/step1.jpg",
            "./assets/images/step2.jpg",
            "./assets/images/step3.jpg",
            "./assets/images/step4.jpg",
            "./assets/images/step5.jpg",
            "./assets/images/step6.jpg"
          ]
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "extraMavenRepos": [
              "https://devrepo.kakao.com/nexus/content/groups/public/"
            ]
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash_icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#20284F"
        }
      ],
      [
        "@react-native-kakao/core",
        {
          "nativeAppKey": "e6b1b3f5e6e616b6f75e926f9514ba8e",
          "android": {
            "authCodeHandlerActivity": true
          }
          
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "a99a8002-5afe-4b34-bbab-7c8ef1c9e195"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/a99a8002-5afe-4b34-bbab-7c8ef1c9e195"
    }
  }
}
